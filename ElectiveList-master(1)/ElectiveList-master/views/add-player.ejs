<% include partials/header.ejs %>
    <div class="container">
        <% if (message != '') { %>
            <p class="text-center text-danger"><%= message %></p>
        <% } %>

        <h3> Elective Details </h3>

        <form class="add-player-form" action="" method="post" enctype="multipart/form-data">
            <div class="form-row">
                <div class="form-group col-md-4">
                    <select id="last-name" name="course_name" class="form-control" required>
                        <option selected disabled>Choose Elective Subject</option>
                        <option>DBMS</option>
                        <option>EADD</option>
                        <option>Big Data Technologies</option>
                        <option>AI</option>
                    </select>
                </div>
            </div>

            <button type="submit" class="btn btn-primary float-right">Add Elective</button>
        </form>
    </div>
</div>
</body>
</html>
